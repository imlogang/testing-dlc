{
    "name": "mdxpatientapi",
    "version": "1.5.6",
    "description": "RESTful, FHIR-friendly API that allows any MEDNAX system to interact with clinical data",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/mednax-it/mdx-patient-api.git"
    },
    "author": "",
    "license": "ISC",
    "main": "lib/index.js",
    "directories": {
      "lib": "lib",
      "test": "test"
    },
    "scripts": {
      "start": "node server",
      "test": "lab",
      "test:mocha": "npm run test:mocha:unit && npm run test:mocha:integration",
      "test:mocha:unit": "mocha --config .mocharc.json --loader=testdouble __tests__/unit",
      "test:mocha:unit:cov": "c8 mocha --config .mocharc.json --loader=testdouble __tests__/unit",
      "test:mocha:unit:watch": "mocha --loader=testdouble __tests__/unit --parallel --watch",
      "test:mocha:integration": "JWT_STRATEGY=mock FOA_BASE_URL=https://fake_url NODE_ENV=test mocha --config .mocharc.integration.json --timeout 5000 --loader=testdouble --exit __tests__/integration",
      "test:system:local": "FRISBY_ENV=local bin/system-tests.sh",
      "test:system:dev": "FRISBY_ENV=dev bin/system-tests.sh",
      "test:system:stage": "FRISBY_ENV=stage bin/system-tests.sh",
      "test:system:dev-smoke": "FRISBY_ENV=dev bin/dev-deploy-smoke-test.sh",
      "test:unit": "lab --pattern unit",
      "test:post-deploy": "lab --pattern 'index|integration|post-deploy' --coverage-exclude server",
      "test:jobsystem": "lab -v ./jobsystem/test",
      "test:list": "lab -vd || true",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix"
    },
    "dependencies": {
      "@azure/storage-blob": "^12.5.0",
      "@hapi/boom": "9.x.x",
      "@hapi/code": "8.x.x",
      "@hapi/glue": "8.x.x",
      "@hapi/hapi": "^20.2.0",
      "@hapi/inert": "^6.0.3",
      "@hapi/jwt": "2.x.x",
      "@hapi/vision": "6.x.x",
      "@mednax/mednaxdoc": "github:mednax-it/mednaxdoc",
      "@opentelemetry/api": "^1.0.1",
      "applicationinsights": "^1.8.10",
      "chance": "^1.1.8",
      "confidence": "5.x.x",
      "dotenv": "8.2.0",
      "form-data": "3.x.x",
      "haute-couture": "3.x.x",
      "hpal-debug": "1.x.x",
      "joi": "17.x.x",
      "lodash": "4.17.21",
      "moment": "^2.29.4",
      "node-fetch": "^2.6.7",
      "schmervice": "1.x.x",
      "toys": "2.x.x",
      "uuid": "8.3.2",
      "yargs": "16.2.0"
    },
    "devDependencies": {
      "@hapi/eslint-config-hapi": "13.x.x",
      "@hapi/eslint-plugin-hapi": "4.x.x",
      "@hapi/lab": "23.x.x",
      "babel-eslint": "10.x.x",
      "c8": "7.x.x",
      "chai": "4.x.x",
      "chai-as-promised": "7.x.x",
      "eslint": "7.x.x",
      "frisby": "^2.1.3",
      "hpal": "2.x.x",
      "mocha": "^9.2.0",
      "nock": "^13.1.3",
      "nodemon": "^2.0.13",
      "oauth2-mock-server": "^3.2.0",
      "sinon": "10.x.x",
      "testdouble": "3.x.x"
    }
  }
  